{
  "name": "esbuild-plugin-browserslist",
  "description": "Configure esbuild's target based on a browserslist query",
  "version": "0.8.1",
  "author": "Nihal Gonsalves <nihal@nihalgonsalves.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/nihalgonsalves/esbuild-plugin-browserslist.git"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "typecheck": "tsc",
    "build": "rm -rf dist && tsc --project tsconfig.build.json",
    "test": "jest",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src/",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "prepare": "husky install",
    "prepublishOnly": "yarn build && yarn lint && yarn test"
  },
  "peerDependencies": {
    "browserslist": "^4.21.8",
    "esbuild": "~0.18.2"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@nihalgonsalves/esconfig": "^0.3.0",
    "@types/debug": "^4.1.8",
    "@types/jest": "^29.5.2",
    "@types/node": "20",
    "browserslist": "^4.21.9",
    "esbuild": "~0.18.11",
    "eslint": "^8.44.0",
    "eslint-plugin-jest": "^27.2.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  }
}
